\n","w  = np.random.normal(1,0,(train_x.shape[1], 100))\n","w0 = np.zeros(100)\n","v  = np.random.normal(1,0,(100,2))\n","v0 = np.zeros(2)\n","u  = np.random.normal(1,0,(2,1))\n","u0 = np.zeros(1)\n","\n","lr    = 0.1/train_t.size\n","iters = 10\n","\n","print(train_x.shape)\n","print(w0.shape)\n","\n","for i in range(iters):\n","  #forward pass\n","  xhat = np.matmul(train_x, w) + w0\n","  h    = tanh(xhat)\n","  hhat = np.matmul(h, v) + v0\n","  g    = tanh(hhat)\n","  ghat = np.matmul(g, u) + u0\n","  y    = sigmoid(ghat)\n","\n","  pred = np.where(y>0.5, 1,0)\n","  acc  = np.mean(np.where(pred==train_t, 1, 0))\n","  print(\"Iteration Number: {0}. Accuracy: {1}\".format(i+1, acc))\n","\n","  #backwards pass\n","  dCdGhat = y-train_t.reshape(train_t.size,1)\n","  dCdG    = np.matmul(dCdGhat, np.transpose(u))\n","  dCdU    = np.matmul(np.transpose(g), dCdGhat)\n","  dCdU0   = np.matmul(np.ones((1, dCdGhat.shape[0])), dCdGhat).T\n","\n","  dCdHhat = (1-g**2) * dCdG \n","  dCdH    = np.matmul(dCdHhat, np.transpose(v)) \n","  dCdV    = np.matmul(np.transpose(h), dCdHhat)\n","  dCdV0   = np.matmul(np.ones((1, dCdHhat.shape[0])), dCdHhat).T\n","\n","  dCdXhat = (1 - h**2) * dCdH\n","  dCdX    = np.matmul(dCdXhat, np.transpose(w))\n","  dCdW    = np.matmul(np.transpose(train_x), dCdXhat)\n","  dCdW0   = np.matmul(np.ones((1, dCdXhat.shape[0])), dCdXhat).T\n","\n","  w  = w - (lr*dCdW)\n","  w0 = (w0.reshape(w0.shape[0], 1) - (lr*dCdW0)).reshape(w0.shape[0])\n","  v  = v - (lr*dCdV)\n","  v0 = (v0.reshape(v0.shape[0], 1) - (lr*dCdV0)).reshape(v0.shape[0])\n","  u  = u - (lr*dCdU)\n","  u0 = (u0.reshape(u0.shape[0], 1) - (lr*dCdU0)).reshape(u0.shape[0])\n","\n","xhat = np.matmul(test_x, w) + w0\n","h    = tanh(xhat)\n","hhat = np.matmul(h, v) + v0\n","g    = tanh(hhat)\n","ghat = np.matmul(g, u) + u0\n","y    = sigmoid(ghat)\n","\n","pred = np.where(y>0.5, 1,0)\n","acc  = np.mean(np.where(pred==test_t, 1, 0))\n","CE   = -test_t*np.log(y) - (1-test_t)*np.log(1-y)\n","print(\"Test accuracy: {0}. Test Loss: {1}\". format(acc, np.mean(CE)))"],"execution_count":null,"outputs":[{"output_type":"stream","text":["(9444, 784)\n","(100,)\n","Iteration Number: 1. Accuracy: 0.4776577721304532\n","(1, 1)\n","Iteration Number: 2. Accuracy: 0.4776577721304532\n","(1, 1)\n","Iteration Number: 3. Accuracy: 0.4776577721304532\n","(1, 1)\n","Iteration Number: 4. Accuracy: 0.4776577721304532\n","(1, 1)\n","Iteration Number: 5. Accuracy: 0.4776577721304532\n","(1, 1)\n","Iteration Number: 6. Accuracy: 0.4776577721304532\n","(1, 1)\n","Iteration Number: 7. Accuracy: 0.4776577721304532\n","(1, 1)\n","Iteration Number: 8. Accuracy: 0.4776577721304532\n","(1, 1)\n","Iteration Number: 9. Accuracy: 0.4776577721304532\n","(1, 1)\n","Iteration Number: 10. Accuracy: 0.4776577721304532\n","(1, 1)\n","(100,)\n","Test accuracy: 0.4821621621621622. Test Loss: 0.8284626355684125\n"],"name":"stdout"}]},{"cell_type":"markdown","metadata":{"id":"hmCcW8v7XJKo"},"source":["g)"]},{"cell_type":"code","metadata":{"id":"hyor1qAlXKOG"},"source":["'''\n","I don't Know\n","'''"],"execution_count":null,"outputs":[]}]}